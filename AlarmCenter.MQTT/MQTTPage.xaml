<gwccMQTT:GWPageContentControl  xmlns:gwccMQTT="clr-namespace:AlarmCenter.GWWpfCustomControlLibrary;assembly=GWWpfCustomControlLibrary" x:Class="AlarmCenter.MQTT.MQTTPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AlarmCenter.MQTT"
             mc:Ignorable="d" 
                                HeaderText="MQTT协议"
             d:DesignHeight="450" d:DesignWidth="800"
                                HorizontalScrollBarVisibility="Disabled"
                                VerticalScrollBarVisibility="Disabled">
    <Grid>
        <DataGrid Name="dg">
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding Message}" Width="100" Header="Mqtt通信消息"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</gwccMQTT:GWPageContentControl>
